    <link name="hip">
        <inertial>
            <mass value="10"/>
            <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
        </inertial>
        <visual>
            <geometry>
                <sphere radius="0.05"/>
            </geometry>
            <material name="green">
                <color rgba="0 1 0 1"/>
            </material>
        </visual>
    </link>
    <link name="hip2pin"/>
    <link name="pin2yaw"/>
    <link name="left_upper_leg">
        <visual>
            <origin xyz="0 0 -0.25"/>
            <geometry>
                <cylinder length="0.5" radius="0.0075"/>
            </geometry>
            <material name="red">
                <color rgba="1 0 0 1"/>
            </material>
        </visual>
    </link>
    <link name="left_upper_leg_mass">
