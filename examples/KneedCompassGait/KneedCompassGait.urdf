    <link name="right_lower_leg_mass">
        <visual>
            <origin xyz="0 0 -0.25"/>
            <geometry>
                <sphere radius="0.025"/>
            </geometry>
            <material name="blue">
                <color rgba="0 0 1 1"/>
            </material>
        </visual>
        <inertial>
            <origin xyz="0 0 -0.25"/>
            <mass value="2.5"/>
            <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
        </inertial>
    </link>
    <joint name="right_lower_leg_mass_weld" type="fixed">
        <parent link="right_lower_leg"/>
        <child link="right_lower_leg_mass"/>
    </joint>
    <joint name="left_leg_weld" type="fixed">
        <parent link="hip"/>
        <child link="left_upper_leg"/>
    </joint>
    <joint name="hip_roll" type="continuous">
        <parent link="hip"/>
        <child link="hip2pin"/>
        <axis xyz="1 0 0"/>
        <limit effort="200" velocity = "100"/>
    </joint>
    <joint name="hip_pin" type="continuous">
        <parent link="hip2pin"/>
        <child link="pin2yaw"/>
        <axis xyz="0 1 0"/>
        <limit effort="200" velocity="100"/>
    </joint>
    <joint name="left_knee_pin" type="revolute">
        <parent link="left_upper_leg"/>
        <child link="left_lower_leg"/>
        <axis xyz="0 1 0"/>
        <origin xyz="0 0 -0.5"/>
        <limit upper = "0" lower="-3.14" effort="200" velocity="100"/>
    </joint>
    <joint name="hip_yaw" type="continuous">
        <parent link="pin2yaw"/>
        <child link="right_upper_leg"/>
        <axis xyz="0 0 1"/>
        <limit effort="200" velocity="100"/>
    </joint>
    <joint name="right_knee_pin" type="revolute">
        <parent link="right_upper_leg"/>
        <child link="right_lower_leg"/>
        <axis xyz="0 1 0"/>
        <origin xyz="0 0 -0.5"/>
        <limit upper="0" lower="-3.14" effort="200" velocity="100"/>
    </joint>
    <transmission name="hip_1trans" type="SimpleTransmission">
        <actuator name="hip_roll_torque"/>
        <joint name="hip_roll"/>
        <mechanicalReduction>1</mechanicalReduction>
    </transmission>
    <transmission name="hip_2trans" type="SimpleTransmission">
        <actuator name="hip_pin_torque"/>
        <joint name="hip_pin"/>
        <mechanicalReduction>1</mechanicalReduction>
    </transmission>
    <transmission name="left_knee_trans" type="SimpleTransmission">
        <actuator name="left_knee_torque"/>
        <joint name="left_knee_pin"/>
        <mechanicalReduction>1</mechanicalReduction>
    </transmission>
    <transmission name="hip_3trans" type="SimpleTransmission">
        <actuator name="hip_yaw_torque"/>
        <joint name="hip_yaw"/>
        <mechanicalReduction>1</mechanicalReduction>
    </transmission>
    <transmission name="right_knee_trans" type="SimpleTransmission">
        <actuator name="right_knee_torque"/>
        <joint name="right_knee_pin"/>
        <mechanicalReduction>1</mechanicalReduction>
    </transmission>
